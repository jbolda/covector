{
  "pkgManagers": {
    "rust": {
      "version": true,
      "prepublish": "cargo package --no-verify",
      "publish": "echo publishing ${ pkg.pkg } would happen here"
    },
    "javascript": {
      "version": true,
      "publish": "echo publishing ${ pkg.pkg } would happen here"
    }
  },
  "packages": {
    "tauri.js": {
      "path": "./cli/tauri.js",
      "manager": "javascript",
      "dependencies": ["tauri"]
    },
    "tauri-bundler": {
      "path": "./cli/tauri-bundler",
      "manager": "rust"
    },
    "tauri": {
      "path": "./tauri",
      "manager": "rust",
      "dependencies": ["tauri-api"]
    },
    "tauri-api": {
      "path": "./tauri-api",
      "manager": "rust",
      "dependencies": ["tauri-utils"]
    },
    "tauri-utils": {
      "path": "./tauri-utils",
      "manager": "rust"
    },
    "tauri-updater": {
      "path": "./tauri-updater",
      "manager": "rust",
      "publish": false,
      "getPublishedVersion": false
    }
  }
}
