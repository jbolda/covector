{
  "compilerOptions": {
    "composite": true,
    "strict": true,
    "module": "commonjs",
    "target": "es6",
    "pretty": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "checkJs": false,
    "preserveSymlinks": true,
    "skipLibCheck": true,
    "declaration": true,
    "declarationMap": true
  }
}
